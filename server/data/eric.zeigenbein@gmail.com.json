      "plotPoints": [
        {
          "id": 1740136563433,
          "text": "Light floods the universe",
          "notes": [
            {
              "blocks": [
                {
                  "key": "cfnea",
                  "text": "The light of stars is ubiquitous, connecting vast voids of space.",
                  "type": "unordered-list-item",
                  "depth": 0,
                  "inlineStyleRanges": [],
                  "entityRanges": [],
                  "data": {}
                },
                {
                  "key": "eoka5",
                  "text": "We start to see the spiral far before we look out in to space -- it is written in every particle of us, the same spiral.",
                  "type": "unordered-list-item",
                  "depth": 0,
                  "inlineStyleRanges": [],
                  "entityRanges": [],
                  "data": {}
                }
              ],
              "entityMap": {}
            }
          ]
        }
      ],
